{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "import"
    ],
    "rules": {
        "no-debugger": 0,
        "no-unused-vars": [
            2,
            {
                "argsIgnorePattern": "res|next|^err"
            }
        ],
        "arrow-body-style": [
            2,
            "as-needed"
        ],
        "arrow-parens": [
            2,
            "as-needed"
        ],
        "no-unused-expressions": [
            2,
            {
                "allowTaggedTemplates": true
            }
        ],
        "no-param-reassign": [
            2,
            {
                "props": false
            }
        ],
        "no-console": 0,
        "no-case-declarations": "off",
        "import/prefer-default-export": 0,
        "import": 0,
        "func-names": 0,
        "import/no-named-as-default": "off",
        "max-len": 0,
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "radix": 0,
        "import/no-named-as-default-member": 0,
        "no-shadow": [
            2,
            {
                "hoist": "all",
                "allow": [
                    "resolve",
                    "reject",
                    "done",
                    "next",
                    "err",
                    "error"
                ]
            }
        ],
        "eqeqeq": 2,
        "prettier/prettier": [
            "error",
            {
                "printWidth": 126,
                "trailingComma": "all",
                "singleQuote": true,
                "tabWidth": 2,
                "useTabs": false,
                "semi": false,
                "bracketSpacing": true,
                "jsxBracketSameLine": true,
                "arrowParens": "avoid"
            }
        ]
    }
}